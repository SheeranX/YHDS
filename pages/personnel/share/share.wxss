/* pages/personnel/share/share.wxss */
.well-paper{
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
}
.well-img{
  width: 100%;
  height: 100%;
}
.txt-info{
  position: absolute;
  z-index: 999;
  top: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  color: #25667b;
  width: 600rpx;
}

.padding-right{
  padding-right: 100rpx;
}

.link-btn{
  position: absolute;
  bottom: 15%;
  left:50%;
  transform: translateX(-50%);
  width: 400rpx;
}
.remind-info{
  position: absolute;
  z-index: 999;
  top: 50%;
  width: 400rpx;
  left: 50%;
  transform: translate(-50%,-55%);
  background: rgba(0, 0,0 , .5);
  border-radius: 10rpx;
  padding: 45rpx;
  color: #fff;
}